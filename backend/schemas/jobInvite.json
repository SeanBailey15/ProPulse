{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://propulse.com/jobInvite.schema.json",
  "title": "Invite to Job",
  "description": "For use with user job invite inputs",
  "type": "object",
  "properties": {
    "invited": {
      "type": "string",
      "minLength": 6,
      "maxLength": 40,
      "format": "email"
    },
    "privilege": {
      "type": "string",
      "minLength": 1,
      "maxLength": 5
    }
  },
  "additionalProperties": false,
  "required": ["invited", "privilege"]
}
