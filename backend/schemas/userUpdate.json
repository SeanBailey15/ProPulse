{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://propulse.com/userUpdate.schema.json",
  "title": "Update User",
  "description": "Update a user in the database",
  "type": "object",
  "properties": {
    "email": {
      "type": "string",
      "minLength": 6,
      "maxLength": 40,
      "format": "email"
    },
    "firstName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "lastName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "phone": {
      "type": "string",
      "minLength": 10,
      "maxLength": 17
    },
    "organization": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "title": {
      "type": "string",
      "minLength": 5,
      "maxLength": 30
    },
    "profileImg": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": []
}
